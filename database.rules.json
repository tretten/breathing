{
  "rules": {
    "rooms": {
      "together": {
        "$presetId": {
          ".read": true,
          "online": {
            "$clientId": {
              ".write": true
            }
          },
          "status": {
            ".write": true
          },
          "startTimestamp": {
            ".write": true
          },
          "voiceSignaling": {
            "$signalingId": {
              ".write": true,
              "iceCandidates": {
                ".write": true
              }
            }
          }
        }
      }
    },
    "presence": {
      ".read": true,
      "$clientId": {
        ".write": true
      }
    }
  }
}
